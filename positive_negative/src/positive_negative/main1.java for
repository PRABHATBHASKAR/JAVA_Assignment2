package positive_negative;

public class main1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int i;
      int sum = 0;
      for(i=1; i <= 10; i++) { 
    	  sum = sum + i;
      }
        System.out.println("the sum of 1st 10 natural nos is:"+sum);
	}

}
